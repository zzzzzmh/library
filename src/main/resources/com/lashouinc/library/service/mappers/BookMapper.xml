<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lashouinc.library.service.mappers.BookMapper">
<insert id="add" parameterType="com.lashouinc.library.service.domain.Book" useGeneratedKeys="true" keyProperty="id">
	insert into book(title,subtitle,origin_title,alt_title,pubdate,publisher,img,binding,douban_id,isbn10,isbn13,price,pages,wanted) 
	 values (#{title},#{subtitle},#{originTitle},#{altTitle},#{pubdate},#{publisher},#{img},#{binding},#{doubanId},#{isbn10},#{isbn13},#{price},#{pages},0);
</insert>
</mapper>